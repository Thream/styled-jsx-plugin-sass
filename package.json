{
  "name": "@styled-jsx/plugin-sass",
  "version": "0.0.0-development",
  "description": "Plugin to add Sass support to styled-jsx",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "styled-jsx-plugin",
    "styled-jsx",
    "sass",
    "scss",
    "css-in-js"
  ],
  "author": "Marton Lederer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thream/styled-jsx-plugin-sass.git"
  },
  "bugs": {
    "url": "https://github.com/Thream/styled-jsx-plugin-sass/issues"
  },
  "homepage": "https://github.com/Thream/styled-jsx-plugin-sass#readme",
  "standard": {
    "envs": [
      "node",
      "mocha"
    ]
  },
  "scripts": {
    "lint:commit": "commitlint",
    "lint:editorconfig": "editorconfig-checker",
    "lint:markdown": "markdownlint '**/*.md' --dot --ignore node_modules",
    "lint:javascript": "standard",
    "release": "semantic-release",
    "test": "mocha test.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "strip-indent": "3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "editorconfig-checker": "4.0.2",
    "husky": "7.0.4",
    "markdownlint-cli": "0.30.0",
    "mocha": "9.1.3",
    "pinst": "2.1.6",
    "sass": "1.48.0",
    "semantic-release": "18.0.1",
    "standard": "16.0.4"
  },
  "peerDependencies": {
    "sass": "*"
  }
}
